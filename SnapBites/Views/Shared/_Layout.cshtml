    <link rel="stylesheet" href="~/css/estilo_login.css">
    <link rel="stylesheet" href="~/css/home.css">
    <link rel="stylesheet" href="~/css/Interesses.css">
    <link rel="stylesheet" href="~/css/Meu_perfil.css">
    <link rel="stylesheet" href="~/css/Publicacao.css">
    <link rel="stylesheet" href="~/css/Publicar.css">
    <link rel="stylesheet" href="~/css/reset.css">
    <link href="~/css/bootstrap-5.3.0-alpha3-dist/css/bootstrap-grid.css" />
    <link href="~/css/bootstrap-5.3.0-alpha3-dist/css/bootstrap-grid.css.map" />
    <link href="~/css/bootstrap-5.3.0-alpha3-dist/css/bootstrap-grid.min.css" />
