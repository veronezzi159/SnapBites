            crossorigin="anonymous"></script>
</head>
<body>
    <nav class="navbar fixed-top navbar-expand cor1">
        <div class="container-fluid">
            <a href="home.html" class="navbar-brand">
                <img src="imagens/Logo Branca.svg" height="65px" width="65px" style="margin-top: -15px;  margin-bottom: -15px;">
            </a>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto">
                    <li class="nav-item"><a class="nav-link text-light active" href="#home">Home</a></li>
                    <li class="nav-item"><a class="nav-link text-light" asp-controller="Prato" asp-action="Index">Feed</a></li>
                    <li class="nav-item"><a class="nav-link text-light" href="#tela-sobre">Sobre</a></li>
                    <li class="nav-item"><a class="nav-link text-light" href="#tela-contato">Contato</a></li>
                </ul>
                <form class="d-flex" role="search">
                    <input class="form-control me-2" type="search" placeholder="Pesquisar" aria-label="Search">
                    <button class="btn btn-light" type="submit">
                        <img src="imagens/Bt-Search.svg" height="25px" width="25px"
                             alt="">
                    </button>
                </form>
            </div>
        </div>
    </nav>
    <div>
        @RenderBody()
    </div>
    <footer>
        <p>&copy; 2023 SnapBites. Todos os direitos reservados.</p>
